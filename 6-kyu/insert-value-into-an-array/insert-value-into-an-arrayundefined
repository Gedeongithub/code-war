xxxxxxxxxx 1const expect = require("chai").expect;2​3​4describe("Simple inserts", function(){5      var tests = [6        [[1,2,3],0,0, [0,1,2,3]],7        [['1',2,4],1,'s', ['1','s',2,4]],8        [[true,true,true],2,false,[true,true,false,true]]9      ];10      11      tests.forEach(a=>{12        it("Should insert as intended", () => {13         expect(a[0].insert(a[1],a[2])).to.eql(a[3]);          14        });15      });16});17​